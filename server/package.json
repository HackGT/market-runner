{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*' --exec 'ts-node' src/app.ts",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "prisma-generate": "prisma format && prisma generate",
    "seed": "prisma db seed --preview-feature",
    "start": "node dist/app.js",
    "start-migrate": "yarn migrate:deploy && yarn start"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/node": "^16.10.2",
    "@types/passport": "^1.0.7",
    "nodemon": "^2.0.13",
    "prisma": "3.1.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@prisma/client": "3.1.1",
    "@quixo3/prisma-session-store": "^3.1.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "http-status-codes": "^2.1.4",
    "passport": "^0.5.0",
    "passport-ground-truth": "^1.0.3"
  }
}
