{
  "name": "server",
  "version": "1.0.0",
  "main": "./src/app.ts",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*' --exec 'ts-node' src/app.ts",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "prisma-generate": "prisma format && prisma generate",
    "seed": "prisma db seed --preview-feature",
    "start": "node dist/app.js",
    "start-migrate": "yarn migrate:deploy && yarn start",
    "lint": "eslint src/ --fix; stylelint src/**/*.css --fix; prettier src/ --write"
  },
  "devDependencies": {
    "@hex-labs/eslint-config": "^1.1.13",
    "@hex-labs/prettier-config": "^1.1.13",
    "@hex-labs/tsconfig": "^1.1.13",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.10.2",
    "@types/passport": "^1.0.7",
    "@types/passport-oauth2": "^1.4.11",
    "eslint": "^8.0.1",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "prisma": "3.1.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@prisma/client": "3.1.1",
    "@quixo3/prisma-session-store": "3.0.1",
    "axios": "^0.23.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "http-status-codes": "^2.1.4",
    "morgan": "^1.10.0",
    "node-fetch": "^3.0.0",
    "passport": "^0.5.0",
    "passport-ground-truth": "^1.0.3"
  },
  "eslintConfig": {
    "extends": "@hex-labs/eslint-config"
  },
  "prettier": "@hex-labs/prettier-config"
}
